<ul class="grid">
  {{#each flags}}
    <li class="flags" data-id={{id}}>
      <img class="img-responsive flags" data-id={{id}} data-original={{imgurl}} name={{name}} />
      <h4 data-id={{id}}>{{name}}</h4>
    </li>
  {{/each}}
</ul>
